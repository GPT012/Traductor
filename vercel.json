{
    "version": 2,
    "builds": [
        {
            "src": "ConFluent_Chrome/website/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "."
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/ConFluent_Chrome/website/$1"
        }
    ]
}